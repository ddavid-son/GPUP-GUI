<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<ScrollPane fitToHeight="true" fitToWidth="true" BorderPane.alignment="CENTER"
            xmlns="http://javafx.com/javafx/8.0.171">
    <content>
        <VBox spacing="10.0">
            <children>
                <Button mnemonicParsing="false" text="Load XML" VBox.vgrow="NEVER">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0"/>
                    </VBox.margin>
                </Button>
                <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                        text="Find path between nodes" textOverrun="CLIP" VBox.vgrow="NEVER">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button mnemonicParsing="false" text="Find if node is in circle" VBox.vgrow="NEVER">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button mnemonicParsing="false" text="Display all related" VBox.vgrow="NEVER">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button mnemonicParsing="false" text="Run task" VBox.vgrow="NEVER">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </content>
</ScrollPane>
